{
    "name": "fuzzyopen",
    "displayName": "Fuzzy Open",
    "description": "Fuzzy Opens a Path using the Command Palette",
    "version": "0.0.1",
    "publisher": "Julwrites",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openPath",
        "onCommand:extension.setRoot",
        "onCommand:extension.registerPath"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
        {
            "command": "extension.openPath",
            "title": "Open Path"
        }, 
        {
            "command": "extension.setRoot",
            "title": "Set Root Path"
        },
        {
            "command": "extension.registerPath",
            "title": "Register Path"
        },
        {
            "command": "extension.deregisterPath",
            "title": "De-Register Path"
        }],
        "configuration": {
            "type": "object",
            "title": "Fuzzy Open Configuration",
            "properties": {
                "fuzzyopen.rootPath": {
                    "type": "string",
                    "title": "Root Path",
                    "default": ""
                }, 
                "fuzzyopen.regPath": {
                    "type": "list",
                    "title": "Registered Paths",
                    "default": []
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "fs": "^0.0.1-security",
        "file-url": "^2.0.0",
        "clone": "^2.0.0",
        "drivelist": "^5.0.0"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}